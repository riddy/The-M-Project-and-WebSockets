{
    "name" : "MySocketTest",
    "mode": "debug",
    "debugLevel": 1,
    "eliminate": false,
    "minify": false,
    "proxies": [
        {
            "baseUrl": "http://localhost:8080",
            "proxyAlias": "Socket"
        },
        {
            "baseUrl": "http://www.google.de",
            "proxyAlias": "googletest"
        }
    ],
    "htmlHeader": [
        "<meta charset=\"utf-8\">",
        "<meta name=\"apple-mobile-web-app-capable\" content=\"yes\">",
        "<meta name=\"apple-mobile-web-app-status-bar-style\" content=\"default\">",
        "<link rel=\"apple-touch-icon\" href=\"theme/images/Icon-72.png\" />",
        "<link rel=\"apple-touch-icon\" href=\"theme/images/Icon.png\" />",
        "<link rel=\"apple-touch-icon\" href=\"theme/images/Icon@2x.png\" />"
    ],
    "application": {
        "version": "0.0.1",
        "debugMode": true,
        "useTransitions": true,
        "enableTouchOverflow": true,
        "keyPrefix": "#m#",
        "keySuffix": "_",
        "timeStampCreated": "created_at",
        "timeStampUpdated": "updated_at",
        "m_id": "m_id"
    },
    "libraries": [
        {
            "name": "Socket",
            "refs": ["socket.io.js"]
        }
    ]
}
